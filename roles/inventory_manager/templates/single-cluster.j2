; hosts and groups

vpc-manager


[zone1]
zone1-az
zone1-apphost1
zone1-bigip1
zone1-bigip2

[vpc]

[vpc:children]
zone1


[azs]
zone1-az


[apphosts]
zone1-apphost1


[bigips]
zone1-bigip1
zone1-bigip2


[zone1-bigip-cluster]
zone1-bigip1
zone1-bigip2


[bigip-clusters]
