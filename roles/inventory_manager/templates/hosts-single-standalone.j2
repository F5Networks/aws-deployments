; ansible "hosts" inventory for {{ env_name }} 
; This is an auto-generated file for this deployment. 

; hosts and groups

vpc-manager


[zone1]
zone1-az
zone1-apphost1
zone1-bigip1
{% if deploy_analytics %}
zone1-analyticshost1
{% endif %}

[vpc]

[vpc:children]
zone1

[azs]
zone1-az

[apphosts]
zone1-apphost1

[bigips]
zone1-bigip1

{% if deploy_analytics %}
[analyticshosts]
zone1-analyticshost1
{% endif %}


[zone1-bigip-cluster]
zone1-bigip1

[bigip-clusters]
zone1-bigip-cluster

[bigip-cluster-seeds]
zone1-bigip1


; group vars

[all:vars]
env_name={{ env_name }}
image_id={{ image_id }}
container_cnt={{ containers_per_app_host }}
deployment_model={{ deployment_model }}
deployment_type={{ deployment_type }}
deploy_analytics={{ deploy_analytics }}
region={{ region }}
vpc_cidr={{ vpc_cidr_prefix }}.0.0/16
key_name={{ ssh_key_name }}

[zone1:vars]
zone_id=zone1
availability_zone="{{ zone }}"
zone_cidr_prefix="{{ vpc_cidr_prefix }}.1"
management_cidr="{{ vpc_cidr_prefix }}.11.0/24"
private_cidr="{{ vpc_cidr_prefix }}.12.0/24"
public_cidr="{{ vpc_cidr_prefix }}.13.0/24"
application_cidr="{{ vpc_cidr_prefix }}.14.0/24"


[localhost:vars]
ansible_connection=local
ansible_python_interpreter="/usr/bin/env python"
